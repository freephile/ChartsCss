{
	"name": "ChartsCss",
	"version": "0.1.0",
	"author": [
		"[https://github.com/freephile/ Greg Rundlett]"
	],
	"url": "https://github.com/freephile/ChartsCss",
	"descriptionmsg": "chartscss-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"MessagesDirs": {
		"ChartsCss": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"ChartsCss\\ChartsCssHooks": "includes/ChartsCssHooks.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"ChartsCss\\ChartsCssHooks::onParserFirstCallInit"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "ChartsCss"
	},
	"ResourceModules": {
		"ext.chartscss": {
			"styles": [
				"modules/ext.chartscss/charts.min.css"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"manifest_version": 2
}
